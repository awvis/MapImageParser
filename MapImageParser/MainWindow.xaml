<Controls:MetroWindow x:Class="MapImageParser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MapImageParser" Icon="./ico.ico"
        xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        mc:Ignorable="d"
        Title="Map Image Parser" Height="350" Width="525" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="button" Content="Upload CSV" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" Width="75" Click="button_Click" Foreground="#FF676767"/>
        <Image x:Name="image" Source="./logo.jpg" HorizontalAlignment="Left" Height="175" Margin="270,70,0,0" VerticalAlignment="Top" Width="204"/>
        <Label x:Name="label" Content="..." HorizontalAlignment="Left" Height="35" Margin="10,260,0,0" VerticalAlignment="Top" Width="120" Background="Black" Foreground="#FF00FF17"/>

    </Grid>
</Controls:MetroWindow>
